#include<iostream>
using namespace std;
int main()
 {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int i,n;
	    cin>>n;
	    int a[102]={0};
	    int x;
	    for(i=0;i<n;i++)
	    {
	        cin>>x;
	        a[x]++;
	    }
	    for(i=1;i<=n;i++)
	    cout<<a[i]<<" ";
	    cout<<endl;
	    
	}
	return 0;
}
