#include<iostream>
#include<algorithm>
using namespace std;
int main()
 {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int i,n,k;
	    cin>>n>>k;
	    int a[n];
	    for(i=0;i<n;i++)
	    cin>>a[i];
	    sort(a,a+n);
	    i=n-1;
	    while(k--)
	    {
	        cout<<a[i]<<" ";
	        i--;
	    }
	    cout<<endl;
	}
	return 0;
}
