#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n,i,k;
	    cin>>n;
	    int arr[n];
	    for(i=0;i<n;i++)
	      cin>>arr[i];
	      cin>>k;
	      int ind=0,flag=0;
	    for(i=0;i<n;i++)
	    {
	        if(arr[i]==k)
	        {
	            flag=1;
	            break;
	        }
	        else
	        ind++;
	    }
	    if(flag==1)
	      cout<<ind<<endl;
	    else
	       cout<<"-1"<<endl;
	}
	return 0;
}
